@model IList<Blog.Entities.Entities.Post>

@{
    ViewBag.Title = "Listado";
}

<h2>Blog de mi Web</h2>

@if (Model.Count > 0){
<ul>
    @foreach(var post in Model){
        <li>@Html.RouteLink(post.Title,"Default", new { action = "post", id = @post.Url})</li>
    }
</ul>
}else{
    <p>No hay ningun post para mostrar</p>
}
